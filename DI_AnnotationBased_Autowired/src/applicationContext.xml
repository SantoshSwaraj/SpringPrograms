<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-4.3.xsd
		http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-4.3.xsd">
		
	<context:annotation-config></context:annotation-config>
	<bean id="employee" class="main.model.Employee">
		<property name="empId" value="101"></property>
		<property name="empName" value="Santosh"></property>
	</bean>
	<bean id="address" class="main.model.Address">
		<property name="houseNo" value="2210"></property>
		<property name="houseName" value="Akash-Vila"></property>
		<property name="street" value="Ring Road, MG Marg"></property>
		<property name="city" value="Pune"></property>
	</bean>
</beans>
